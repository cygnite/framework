{
    "name": "cygnite/framework",
    "description": "Cygnite PHP Framework",
    "keywords": ["Cygnite", "Framework"],
    "homepage": "http://www.cygniteframework.com",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/cygnite/framework/issues",
        "source": "https://github.com/cygnite/framework"
    },
    "authors": [
        {
            "name": "Sanjoy Dey",
            "email": "dey.sanjoy0@gmail.com"
        }
    ],
    "require": {
         "php": ">=5.4",
         "twig/twig": "1.*",
         "symfony/console": "2.6.*",
         "tracy/tracy": "2.3.*",
         "ircmaxell/password-compat": "~1.0",
         "swiftmailer/swiftmailer": "5.4.*"
   },
    "replace": {
        "cygnite/stripe": "self.version" 
	},
   "require-dev": {
       "mockery/mockery": "~0.9.1",
       "phpunit/phpunit": "~4.0",
       "predis/predis": "~1.0"
    },
     "autoload": {
     	"files": [
            "src/Cygnite/Helpers/Support.php"
        ],
        "psr-4": {
            "Cygnite\\": "src/Cygnite/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "dev"
}